{
  "version": 1.3,
  "locale": {
    "source": "en-US",
    "targets": ["es", "fr"]
  },
  "buckets": {
    "json": {
      "include": [
        "./knock/translations/[locale]/[locale].json",
        "./knock/translations/[locale]/*.[locale].json"
      ]
    }
  },
  "$schema": "https://lingo.dev/schema/i18n.json"
}
